set nocompatible
filetype indent on
filetype on
filetype plugin on
filetype plugin indent on
set ai
set si
set ts=2
set sw=2
set softtabstop=2
set autoread
set expandtab
set hidden
set infercase
set lazyredraw
set novisualbell
set shiftround
set showcmd
set showmatch
set smartcase
set smarttab
set title
set wildmenu
set backspace=indent,eol,start
set selectmode=mouse
set mouse=a
color simplewhite

let mapleader = "\<Space>"
nnoremap <Leader>w :w<CR>
